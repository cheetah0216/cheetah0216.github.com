<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>liufei</title>
 <link href="http://liufei.name/atom.xml" rel="self"/>
 <link href="http://lifuei.name"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://liufei.name</id>
 <author>
   <name>liufei</name>
   <email>flying0216@gmail.com</email>
 </author>

{% assign posts_all = site.posts %}
{% if site.JB.posts_all.provider == "custom" %}
  {% include custom/posts_all %}
{% else %}
  {% for post in posts_all %}
  
 <entry>
   <title>{{ post.title }}</title>
   <link href="{{ BASE_PATH }}{{ post.url }}"/>h
   <updated>{{ post.date }}</updated>
   <content type="html">{{ post.description }}</content>
 </entry>
  {% endfor %}
{% endif %}
{% assign posts_all = nil %}

</feed>

